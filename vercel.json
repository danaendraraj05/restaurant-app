{
    "builds": [{
        "src": "restaurant/restaurant/wsgi.py",
        "use": "@ardnt/vercel-python-wsgi",
        "config": { 
            "maxLambdaSize": 15000000,  // 15 MB
            "runtime": "python3.9"  
        }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "restaurant/restaurant/wsgi.py"
        }
    ]
}
